{
  "name": "alltweets",
  "version": "0.1.3",
  "main": "./lib/index.js",
  "bin": {
    "alltweets": "./lib/cli.js"
  },
  "author": "ivgtr <ivgtr.me@gmail.com> (https://github.com/)",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ivgtr/node-alltweets.git"
  },
  "homepage": "https://github.com/ivgtr/node-alltweets#readme",
  "keywords": [
    "Twitter",
    "cli"
  ],
  "scripts": {
    "start": "tsc -w",
    "rename:cli": "node -e \"require('fs').rename('lib/index.js', 'lib/cli.js', function(err) { if (err) console.log(err); console.log('File successfully renamed!') })\"",
    "prebuild": "rimraf lib",
    "build:cli": "ncc build src/cli.ts -o lib -m && yarn rename:cli",
    "build:module": "ncc build src/index.ts -o lib -m",
    "build": "yarn build:cli && yarn build:module",
    "prepublishOnly": "yarn build"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "chalk": "^4.1.0",
    "commander": "^6.2.0",
    "dayjs": "^1.9.7",
    "dotenv": "^8.2.0",
    "js-yaml": "^3.14.1",
    "meow": "^8.0.0",
    "ora": "^5.1.0",
    "pjson": "^1.0.9",
    "twitter-d": "^0.4.0",
    "update-notifier": "^5.0.1"
  },
  "devDependencies": {
    "@types/js-yaml": "^3.12.5",
    "@types/node": "^14.14.11",
    "@types/update-notifier": "^5.0.0",
    "@vercel/ncc": "^0.25.1",
    "eslint": "^7.15.0",
    "eslint-config-sumikko": "^0.1.2",
    "husky": "^4.3.5",
    "typescript": "^4.1.2"
  },
  "license": "MIT"
}
