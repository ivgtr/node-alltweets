{
  "name": "alltweets",
  "version": "1.4.1",
  "description": "Downloads someone's all tweets",
  "type": "module",
  "module": "./lib/index.cjs",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "bin": {
    "alltweets": "./lib/cli.js"
  },
  "engines": {
    "node": ">= 12.2.0"
  },
  "author": {
    "name": "ivgtr",
    "email": "ivgtr.me@gmail.com",
    "url": "https://github.com/ivgtr"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ivgtr/node-alltweets.git"
  },
  "homepage": "https://github.com/ivgtr/node-alltweets#readme",
  "bugs": {
    "url": "https://github.com/ivgtr/node-alltweets/issues"
  },
  "keywords": [
    "twitter",
    "cli",
    "cli-app"
  ],
  "files": [
    "lib",
    "src",
    "configs/error.json",
    "docs/README_EN.md"
  ],
  "scripts": {
    "dev": "node --loader ts-node/esm ./src/cli.ts",
    "start": "tsc -w",
    "type-check": "tsc --noEmit",
    "type-build": "tsc --project ./tsconfig-build.json --emitDeclarationOnly",
    "prebuild": "node -e 'fs.rmSync(`lib`, {recursive:true, force:true})'",
    "build": "rollup -c && npm run type-build",
    "format": "prettier -w ./src/*"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "chalk": "^4.1.1",
    "dotenv": "^10.0.0",
    "js-yaml": "^4.1.0",
    "meow": "10.1.1",
    "ora": "^5.4.1",
    "update-notifier": "^5.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.15.0",
    "@babel/preset-env": "7.15.0",
    "@rollup/plugin-babel": "5.3.0",
    "@rollup/plugin-commonjs": "19.0.2",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "13.0.4",
    "@rollup/plugin-typescript": "8.2.5",
    "@types/js-yaml": "4.0.2",
    "@types/node": "16.0.0",
    "@types/update-notifier": "5.0.1",
    "eslint": "7.31.0",
    "eslint-config-sumikko": "1.2.1",
    "prettier": "2.3.2",
    "rollup": "2.55.1",
    "rollup-plugin-auto-external": "2.0.0",
    "rollup-plugin-preserve-shebang": "1.0.1",
    "rollup-plugin-terser": "7.0.2",
    "ts-node": "10.1.0",
    "twitter-d": "0.4.0",
    "typescript": "4.3.5"
  },
  "license": "MIT"
}
